<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >


        <record model="account.account.type" id="account_type_ordine">
            <field name="name">Conti D'ordine</field>
            <field name="type">other</field>
            <field name="internal_group">off_balance</field>
        </record>


        <!-- STATO PATRIMONIALE CEE-->
        <record id="stato_patrimoniale_cee" model="account.financial.html.report">
            <field name="name">STATO PATRIMONIALE CEE</field>
            <field name="date_range" eval="True"/>
            <field name="analytic" eval="True"/>
            <field name="unfold_all_filter" eval="True"/>
            <field name='parent_id' ref='account.account_reports_legal_statements_menu'/>
        </record>

        <!--        ATTIVO       -->
        <record id="sp_attivo" model="account.financial.html.report.line">
            <field name="name">ATTIVO</field>
            <field name="code">SP_AT</field>
            <field name="financial_report_id" ref="stato_patrimoniale_cee"/>
            <field name="sequence" eval="1"/>
            <field name="level" eval="0" />
        </record>
        <record id="totale_attivo" model="account.financial.html.report.line">
            <field name="name">TOTALE ATTIVITA'</field>
            <field name="code">SP_TOT_ATT</field>
            <field name="financial_report_id" ref="stato_patrimoniale_cee"/>
            <field name="formulas">SP_AT</field>
            <field name="sequence" eval="4"/>
            <field name="level" eval="0" />
        </record>


        <!--        PASSIVO         -->
        <record id="sp_passivo" model="account.financial.html.report.line">
            <field name="name">PASSIVO</field>
            <field name="code">SP_PASS</field>
            <field name="financial_report_id" ref="stato_patrimoniale_cee"/>
            <field name="sequence" eval="2"/>
            <field name="level" eval="0" />
        </record>
        <record id="totale_passivo_sp" model="account.financial.html.report.line">
            <field name="name">TOTALE PASSIVITA' E NETTO</field>
            <field name="code">SP_TOT_PASS</field>
            <field name="financial_report_id" ref="stato_patrimoniale_cee"/>
            <field name="formulas">SP_PASS</field>
            <field name="sequence" eval="5"/>
            <field name="level" eval="0" />
        </record>

        <record id="totale_utile_sp" model="account.financial.html.report.line">
            <field name="name">UTILE/PERDITA D'ESERCIZIO</field>
            <field name="code">SP_UTILE</field>
            <field name="financial_report_id" ref="stato_patrimoniale_cee"/>
            <field name="formulas">- SP_TOT_ATT - SP_TOT_PASS</field>
            <field name="sequence" eval="6"/>
            <field name="level" eval="0" />
        </record>
        <record id="totale_pareggio_sp" model="account.financial.html.report.line">
            <field name="name">TOTALE A PAREGGIO</field>
            <field name="code">SP_PAREGGIO</field>
            <field name="financial_report_id" ref="stato_patrimoniale_cee"/>
            <field name="formulas">- SP_UTILE - SP_TOT_PASS</field>
            <field name="sequence" eval="7"/>
            <field name="level" eval="0" />
        </record>

        <!--        CONTI D'ORDINE         -->
        <record id="sp_conti_ordine" model="account.financial.html.report.line">
            <field name="name">CONTI D'ORDINE</field>
            <field name="code">SP_CONTI_ORDINE</field>
            <field name="financial_report_id" ref="stato_patrimoniale_cee"/>
            <field name="sequence" eval="3"/>
            <field name="level" eval="0" />
        </record>


        <!--        CONTO ECONOMICO         -->
        <record id="conto_economico_cee" model="account.financial.html.report">
            <field name="name">CONTO ECONOMICO CEE</field>
            <!--            <field name="cash_basis" eval="True"/>-->
            <!--            <field name="debit_credit" eval="False"/>-->
            <field name="date_range" eval="True"/>
            <field name="analytic" eval="True"/>
            <field name="unfold_all_filter" eval="True"/>
            <field name='parent_id' ref='account.account_reports_legal_statements_menu'/>
        </record>

        <!--        RICAVI       -->
        <record id="ce_attivo" model="account.financial.html.report.line">
            <field name="name">RICAVI</field>
            <field name="code">CE_AT</field>
            <field name="financial_report_id" ref="conto_economico_cee"/>
            <field name="sequence" eval="1"/>
            <field name="level" eval="0"/>
        </record>
        <record id="totale_ricavi" model="account.financial.html.report.line">
            <field name="name">TOTALE RICAVI</field>
            <field name="code">CE_TOT_ATT</field>
            <field name="financial_report_id" ref="conto_economico_cee"/>
            <field name="formulas">CE_AT</field>
            <field name="sequence" eval="3"/>
            <field name="level" eval="0"/>
        </record>


        <!--        COSTI         -->
        <record id="ce_passivo" model="account.financial.html.report.line">
            <field name="name">COSTI</field>
            <field name="code">CE_PASS</field>
            <field name="financial_report_id" ref="conto_economico_cee"/>
            <field name="sequence" eval="2"/>
            <field name="level" eval="0"/>
        </record>

        <record id="totale_costi_ce" model="account.financial.html.report.line">
            <field name="name">TOTALE COSTI</field>
            <field name="code">CE_TOT_PASS</field>
            <field name="financial_report_id" ref="conto_economico_cee"/>
            <field name="formulas">CE_PASS</field>
            <field name="sequence" eval="4"/>
            <field name="level" eval="0"/>
        </record>

        <record id="totale_utile_ce" model="account.financial.html.report.line">
            <field name="name">UTILE/PERDITA D'ESERCIZIO</field>
            <field name="code">CE_UTILE</field>
            <field name="financial_report_id" ref="conto_economico_cee"/>
            <field name="formulas">CE_TOT_ATT + CE_TOT_PASS</field>
            <field name="sequence" eval="5"/>
            <field name="level" eval="0"/>
        </record>
        <record id="totale_pareggio_ce" model="account.financial.html.report.line">
            <field name="name">TOTALE A PAREGGIO</field>
            <field name="code">CE_PAREGGIO</field>
            <field name="financial_report_id" ref="conto_economico_cee"/>
            <field name="formulas">CE_UTILE - CE_TOT_PASS</field>
            <field name="sequence" eval="6"/>
            <field name="level" eval="0"/>
        </record>

        <!--        STATO PATRIMONIALE ODOO-->
        <record id="account_reports.account_financial_report_bank_view0" model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', '=', ref('account.data_account_type_liquidity')),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_report_receivable0" model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id.type', '=', 'receivable'),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_report_current_assets0"
                model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', '=', ref('account.data_account_type_current_assets')),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_report_prepayements0" model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', '=', ref('account.data_account_type_prepayments')),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_report_fixed_assets_view0"
                model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', '=', ref('account.data_account_type_fixed_assets')),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_report_non_current_assets_view0"
                model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', '=', ref('account.data_account_type_non_current_assets')),('escludi_report_bilancio', '=', False)]"/>
        </record>

        <record id="account_reports.account_financial_report_current_liabilities1"
                model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', 'in', [ref('account.data_account_type_current_liabilities'), ref('account.data_account_type_credit_card')]),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_report_current_liabilities_payable"
                model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id.type', '=', 'payable'),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_report_non_current_liabilities0"
                model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', '=', ref('account.data_account_type_non_current_liabilities')),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_current_year_earnings_line_1"
                model="account.financial.html.report.line">
            <field name="domain" eval="[('account_id.user_type_id', 'in', [
                ref('account.data_account_type_revenue'),
                ref('account.data_account_type_other_income'),
                ref('account.data_account_type_direct_costs'),
                ref('account.data_account_type_expenses'),
                ref('account.data_account_type_depreciation')
            ]),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_current_year_earnings_line_2"
                model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', '=', ref('account.data_unaffected_earnings')),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_previous_year_earnings0"
                model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', '=', ref('account.data_unaffected_earnings')),('escludi_report_bilancio', '=', False)]"/>
        </record>
        <record id="account_reports.account_financial_retained_earnings0" model="account.financial.html.report.line">
            <field name="domain"
                   eval="[('account_id.user_type_id', '=', ref('account.data_account_type_equity')),('escludi_report_bilancio', '=', False)]"/>
        </record>


        <!--    REPORT BILANCIO:   SP + CE -->
        <record id="report_bilancio_cee" model="account.financial.html.report">
            <field name="name">REPORT BILANCIO CEE</field>
            <field name="date_range" eval="True"/>
            <field name="analytic" eval="True"/>
            <field name="unfold_all_filter" eval="True"/>
            <field name='parent_id' ref='account.account_reports_legal_statements_menu'/>
        </record>
        <!--        ATTIVO       -->
        <record id="sp_attivo_bilancio" model="account.financial.html.report.line">
            <field name="name">ATTIVO</field>
            <field name="code">SP_AT_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="sequence" eval="1"/>
            <field name="level" eval="0"/>
        </record>
        <record id="totale_attivo_bilancio" model="account.financial.html.report.line">
            <field name="name">TOTALE ATTIVITA'</field>
            <field name="code">SP_TOT_ATT_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="formulas">SP_AT_BILANCIO</field>
            <field name="sequence" eval="4"/>
            <field name="level" eval="0"/>
        </record>


        <!--        PASSIVO         -->
        <record id="sp_passivo_bilancio" model="account.financial.html.report.line">
            <field name="name">PASSIVO</field>
            <field name="code">SP_PASS_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="sequence" eval="2"/>
            <field name="level" eval="0"/>
        </record>
        <record id="totale_passivo_sp_bilancio" model="account.financial.html.report.line">
            <field name="name">TOTALE PASSIVITA' E NETTO</field>
            <field name="code">SP_TOT_PASS_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="formulas">SP_PASS_BILANCIO</field>
            <field name="sequence" eval="5"/>
            <field name="level" eval="0"/>
        </record>

        <record id="totale_utile_sp_bilancio" model="account.financial.html.report.line">
            <field name="name">UTILE/PERDITA D'ESERCIZIO</field>
            <field name="code">SP_UTILE_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="formulas">- SP_TOT_ATT_BILANCIO - SP_TOT_PASS_BILANCIO</field>
            <field name="sequence" eval="6"/>
            <field name="level" eval="0"/>
        </record>
        <record id="totale_pareggio_sp_bilancio" model="account.financial.html.report.line">
            <field name="name">TOTALE A PAREGGIO</field>
            <field name="code">SP_PAREGGIO_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="formulas">- SP_UTILE_BILANCIO - SP_TOT_PASS_BILANCIO</field>
            <field name="sequence" eval="7"/>
            <field name="level" eval="0"/>
        </record>

        <!--        CONTI D'ORDINE         -->
        <record id="sp_conti_ordine_bilancio" model="account.financial.html.report.line">
            <field name="name">CONTI D'ORDINE</field>
            <field name="code">SP_CONTI_ORDINE_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="sequence" eval="3"/>
            <field name="level" eval="0"/>
        </record>


        <!--        CONTO ECONOMICO         -->

        <!--        RICAVI       -->
        <record id="ce_attivo_bilancio" model="account.financial.html.report.line">
            <field name="name">RICAVI</field>
            <field name="code">CE_AT_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="sequence" eval="1"/>
            <field name="level" eval="0"/>
        </record>
        <record id="totale_ricavi_bilancio" model="account.financial.html.report.line">
            <field name="name">TOTALE RICAVI</field>
            <field name="code">CE_TOT_ATT_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="formulas">CE_AT_BILANCIO</field>
            <field name="sequence" eval="3"/>
            <field name="level" eval="0"/>
        </record>


        <!--        COSTI         -->
        <record id="ce_passivo_bilancio" model="account.financial.html.report.line">
            <field name="name">COSTI</field>
            <field name="code">CE_PASS_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="sequence" eval="2"/>
            <field name="level" eval="0"/>
        </record>

        <record id="totale_costi_ce_bilancio" model="account.financial.html.report.line">
            <field name="name">TOTALE COSTI</field>
            <field name="code">CE_TOT_PASS_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="formulas">CE_PASS_BILANCIO</field>
            <field name="sequence" eval="4"/>
            <field name="level" eval="0"/>
        </record>

        <record id="totale_utile_ce_bilancio" model="account.financial.html.report.line">
            <field name="name">UTILE/PERDITA D'ESERCIZIO</field>
            <field name="code">CE_UTILE_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="formulas">CE_TOT_ATT_BILANCIO + CE_TOT_PASS_BILANCIO</field>
            <field name="sequence" eval="5"/>
            <field name="level" eval="0"/>
        </record>
        <record id="totale_pareggio_ce_bilancio" model="account.financial.html.report.line">
            <field name="name">TOTALE A PAREGGIO</field>
            <field name="code">CE_PAREGGIO_BILANCIO</field>
            <field name="financial_report_id" ref="report_bilancio_cee"/>
            <field name="formulas">CE_UTILE_BILANCIO - CE_TOT_PASS_BILANCIO</field>
            <field name="sequence" eval="6"/>
            <field name="level" eval="0"/>
        </record>


