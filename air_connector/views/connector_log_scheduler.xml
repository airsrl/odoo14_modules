<odoo>
    <data noupdate="1">
        <record id="connector_log_cron_task_exec_sync_product" model="ir.cron">
            <field name="name">Huroos - WP Connector log sync products</field>
            <field name="model_id" ref="model_connector_log"/>
            <field name="state">code</field>
            <field name="code">model.force_sync()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name='interval_number'>15</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="False"/>
        </record>

        <record id="connector_log_cron_task_exec_sync_stock_quantity" model="ir.cron">
            <field name="name">Huroos - WP Connector log tasks sync stock quantity</field>
            <field name="model_id" ref="model_connector_log"/>
            <field name="state">code</field>
            <field name="code">model.task_exec_sync_stock_quantity()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name='interval_number'>5</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="False"/>
        </record>

        <record id="connector_log_cron_task_exec_sync_pricelist" model="ir.cron">
            <field name="name">Huroos - WP Connector log sync pricelist</field>
            <field name="model_id" ref="model_product_pricelist"/>
            <field name="state">code</field>
            <field name="code">model.scheduler_pricelist_upd()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name='interval_number'>8</field>
            <field name='interval_type'>hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="False"/>
        </record>

    </data>
</odoo>
