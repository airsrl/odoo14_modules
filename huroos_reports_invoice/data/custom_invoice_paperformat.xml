<odoo>
    <data noupdate="0">
        <record id="huroos_custom_invoice_paperformat" model="report.paperformat">
            <field name="name">Formato carta custom per i report Fatture Huroos</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">86</field>
            <field name="margin_bottom">55</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">80</field>
            <field name="dpi">90</field>
        </record>
    </data>
</odoo>